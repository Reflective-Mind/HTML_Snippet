{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ],
    "env": {
        "REACT_APP_API_URL": "https://mbti-render.onrender.com",
        "REACT_APP_SOCKET_URL": "https://mbti-render.onrender.com",
        "NODE_ENV": "production"
    }
} 